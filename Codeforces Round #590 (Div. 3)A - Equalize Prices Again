#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define fi ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
#define pb push_back
#define all(a) a.begin(),a.end()
#define i64 long long int
const double pi = 2*acos(0);
const int maxn = (i64)2e5+5;
int main()
{
    fi;
    int q;
    cin>>q;
    while(q--)
    {
        int n,i,s = 0,a;
        cin>>n;
        for(i = 0 ; i<n ; i++)
        {
            cin>>a;
            s += a;
        }
        if(s%n == 0) cout<<s/n<<"\n";
        else cout<<(s/n)+1<<"\n";
    }
return 0;
}
