#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define pb push_back
#define pii pair<int,int>
#define pob pop_back
#define all(a) a.begin(),a.end()
#define fio ios_base::sync_with_stdio(false); cin.tie(0);cout.tie(0);
#define i64 long long int
#define mem(x,y) memset(x,y,sizeof(x))
#define fill(arr,b) fill(arr, arr+sizeof(arr)/sizeof(arr[0]), b)

const double pi = acos(-1.0);
const int maxn = (i64)2e5+5;

int arr[200005];
bool c[200005];
int main()
{
    //freopen("input.txt","r",stdin);
    int n,i,j;
    vector<int> v;
    cin>>n;
    for(i = 1 ; i<=n ; i++)
    {
        cin>>arr[i];
        if(arr[i]!=0) c[arr[i]] = 1;
        else c[arr[i]] = 0;
    }

    for(i = 1,j = 1 ; i<=n ; i++)
    {
        if(arr[i] == 0)
        {
            while(j<=n)
            {
                if(c[j] == 0)
                {
                    if(i!=j)
                    {
                        v.pb(i);
                        arr[i] = j;
                    }
                    else
                    {
                        if(!v.empty())
                        {
                            arr[i] = j;
                            int x = v[v.size()-1];
                            swap(arr[x],arr[i]);
                        }
                        else
                        {
                            v.pb(i);
                            arr[i] = j;
                        }
                    }
                    j++;
                    break;
                }
                else j++;
            }
        }
    }


    if(arr[1] == 1) swap(arr[1],arr[2]);


    for(i = 2 ; i<=n ; i++)
    {
        if(arr[i] == i) swap(arr[i],arr[i-1]);
        cout<<arr[i-1]<<" ";
    }
    cout<<arr[n];



return 0;
}


